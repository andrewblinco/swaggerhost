{
	"swagger": "2.0",
	"info": {
		"title": "Azurewebsites",
		"description": "API for iqumulatetecaladocintelligence.azurewebsites.net",
		"version": "0.0.0"
	},
	"host": "iqumulatetecaladocintelligence.azurewebsites.net",
	"schemes": [
		"https"
	],
	"paths": {
		"/api/analyse": {
			"post": {
				"tags": [
					"Web"
				],
				"description": "Analyse document",
				"operationId": "AnalyseDocument",
				"consumes": [
					"application/json"
				],
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "Document",
						"in": "body",
						"description": "Document",
						"required": true,
						"schema": {
							"$ref": "#/definitions/Document"
						}
					},
					{
						"name": "x-functions-key",
						"in": "header",
						"description": "x-functions-key",
						"required": false,
						"type": "string",
						"x-example": ""
					}
				],
				"responses": {
					"200": {
						"description": "Document Fields",
						"schema": {
							"type": "string"
						}
					}
				},
				"security": []
			}
		},
		"/api/document/{AccessCode}": {
			"post": {
				"tags": [
					"Web"
				],
				"description": "Get Quote Document",
				"operationId": "GetQuoteDocument",
				"consumes": [
					"application/json"
				],
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "AccessCode",
						"in": "path",
						"description": "Access Code",
						"required": true,
						"type": "string"
					},
					{
						"name": "env",
						"in": "query",
						"description": "Environment",
						"required": true,
						"type": "string"
					},
					{
						"name": "x-functions-key",
						"in": "header",
						"description": "x-functions-key",
						"required": false,
						"type": "string",
						"x-example": ""
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"schema": {
							"$ref": "#/definitions/QuoteBase64"
						},
						"examples": {
							"application/json": {
								"content": "JVBERi0x...."
							}
						}
					}
				},
				"security": []
			}
		},
		"/api/Base64": {
			"post": {
				"tags": [
					"Web"
				],
				"description": "Convert file to base64",
				"operationId": "ConvertFileToBase64",
				"consumes": [
					"multipart/form-data"
				],
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "x-functions-key",
						"in": "header",
						"description": "x-functions-key",
						"required": true,
						"type": "string",
						"x-example": ""
					},
					{
						"name": "file",
						"in": "formData",
						"description": "file",
						"required": true,
						"type": "file",
						"x-example": ""
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"schema": {
							"$ref": "#/definitions/FileBase64"
						},
						"examples": {
							"application/json": {
								"content": "JVBERi0x....",
								"name": "AEI Endo Invoice Example.pdf",
								"mimeType": "application/pdf"
							}
						}
					}
				},
				"security": []
			}
		},
		"/api/StartSession": {
			"post": {
				"tags": [
					"Web"
				],
				"description": "Start a Session",
				"operationId": "StartSession",
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "x-functions-key",
						"in": "header",
						"description": "x-functions-key",
						"required": true,
						"type": "string",
						"x-example": ""
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"schema": {
							"$ref": "#/definitions/StartSession"
						},
						"examples": {
							"application/json": {
								"SessionId": "JVBERi0x...."
							}
						}
					}
				},
				"security": []
			}
		},
		"/api/AddFile": {
			"post": {
				"tags": [
					"Web"
				],
				"description": "Adds a File",
				"operationId": "AddFile",
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "Document",
						"in": "body",
						"description": "Document",
						"required": true,
						"schema": {
							"$ref": "#/definitions/AddFile"
						}
					},
					{
						"name": "x-functions-key",
						"in": "header",
						"description": "x-functions-key",
						"required": true,
						"type": "string",
						"x-example": ""
					}
				],
				"responses": {
					"200": {
						"description": "message",
						"schema": {
							"type": "string"
						}
					}
				},
				"security": []
			}
		},
		"/api/EndSession": {
			"post": {
				"tags": [
					"Web"
				],
				"description": "End Session",
				"operationId": "EndSession",
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "x-functions-key",
						"in": "header",
						"description": "x-functions-key",
						"required": true,
						"type": "string",
						"x-example": ""
					},
					{
						"name": "EndSession",
						"in": "body",
						"description": "EndSession",
						"required": true,
						"schema": {
							"$ref": "#/definitions/EndSessionRequest"
						}
					}
				],
			"responses": {
					"200": {
						"description": "JSON response containing ZIP file data. Format varies based on ReturnK2Format parameter.",
						"schema": {
							"$ref": "#/definitions/ZipFileResponse"
						},
						"examples": {
							"application/json": {
								"K2Format": {
									"ZipFile": "<file><n>session_abc123.zip</n><content>UEsFBgAAAAAAAAAAAAAAAAAAAAAAAA==</content></file>"
								},
								"StandardFormat": {
									"ZipFile": "UEsFBgAAAAAAAAAAAAAAAAAAAAAAAA=="
								}
							}
						}
					}
				}
			}
		}
	},
	"definitions": {
		"Document": {
			"description": "Model for Document",
			"required": [
				"fileBase64"
			],
			"properties": {
				"fileBase64": {
					"type": "string",
					"x-example": ""
				},
				"model": {
					"type": "string",
					"x-example": "QuoteInvoicesV2"
				},
				"pages": {
					"type": "string",
					"x-example": "1-2"
				}
			}
		},
		"StartSession": {
			"description": "Model for StartSession",
			"required": [
				"sessionId"
			],
			"properties": {
				"sessionId": {
					"type": "string",
					"x-example": "3f3ccd90-67b5-477d-85cb-043dd588bfad"
				}
			}
		},
		"EndSessionRequest": {
			"type": "object",
			"required": [
				"SessionId",
				"ReturnK2Format"
			],
			"properties": {
				"SessionId": {
					"type": "string",
					"format": "uuid",
					"example": "e096dc2e-10e6-45ee-9d25-709cfdeb6a12"
				},
				"ReturnK2Format": {
					"type": "boolean",
					"example": false
				}
			}
		},
		"ZipFileResponse": {
			"type": "object",
			"required": [
				"ZipFile"
			],
			"properties": {
				"ZipFile": {
					"type": "string",
					"description": "When ReturnK2Format=true: Contains XML string with file metadata. When ReturnK2Format=false: Contains base64-encoded ZIP file data.",
					"example": "UEsFBgAAAAAAAAAAAAAAAAAAAAAAAA=="
				}
			}
		},
		"EndSession": {
			"description": "Model for EndSession",
			"required": [
				"sessionId"
			],
			"properties": {
				"sessionId": {
					"type": "string",
					"x-example": "3f3ccd90-67b5-477d-85cb-043dd588bfad"
				},
				"ReturnK2Format": {
					"type": "boolean"
				}
			}
		},
		"AddFile": {
			"description": "Model for AddFile",
			"required": [
				"sessionId"
			],
			"properties": {
				"sessionId": {
					"type": "string",
					"x-example": "3f3ccd90-67b5-477d-85cb-043dd588bfad"
				},
				"FileXml": {
					"type": "string",
					"x-example": "<file><name>filename.txt</name><content>JVBERi0xLjc</content></file>"
				}
			}
		},
		"QuoteBase64": {
			"description": "Model for QuoteBase64",
			"required": [
				"content"
			],
			"properties": {
				"content": {
					"type": "string",
					"x-example": "JVBERi0x...."
				}
			}
		},
		"FileBase64": {
			"description": "Model for FileBase64",
			"required": [
				"content"
			],
			"properties": {
				"content": {
					"type": "string",
					"x-example": "JVBERi0x...."
				},
				"mimeType": {
					"type": "string",
					"x-example": "application/pdf"
				},
				"name": {
					"type": "string",
					"x-example": "AEI Endo Invoice Example.pdf"
				}
			}
		}
	},
	"securityDefinitions": {},
	"security": [],
	"tags": [
		{
			"name": "Web",
			"description": "Operations about Web"
		}
	]
